<%= form_with url: users_path, method: :get, local:true do |f| %>
    <%= f.label :名前 %>
    <%= f.text_field :search_name %>
    <%= f.label :年齢 %>
    <%= f.text_field :search_age %>
    <%= f.submit '検索' %>
<% end %>
<table>
    <tbody>
        <thead>
            <tr>
                <th>名前</th>
                <th>年齢</th>
            </tr>
        </thead>
        <% @users.each do |user| %>
            <tr>
                <td><%= user.name %></td>
                <td><%= user.age %></td>
            </tr>
        <%end%>
    </tbody>
</table>